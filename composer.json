{
  "name": "pskiba/uri-parser",
  "description": "Simple URI parser",
  "authors": [
    {
      "name": "Przemek Skiba"
    }
  ],
  "require": {
    "php" : ">=7.0.0",
    "slim/slim": "4.x-dev"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^1.9",
    "phpunit/phpunit" : "^6.0"
  },
  "autoload": {
    "psr-4": {
      "Pshemo\\UriParser\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pshemo\\UriParser\\": "tests/"
    }
  },
  "scripts": {
    "phpunit": "phpunit --coverage-text"
  }
}
